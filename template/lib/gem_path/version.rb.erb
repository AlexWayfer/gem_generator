# frozen_string_literal: true
<%
	@project_modules.map.with_index do |module_name, index|
%>
<%= "\t" * index %>module <%= module_name %><%
	end
%>
<%= "\t" * @project_modules.size %>VERSION = '0.0.0'<%
	@project_modules.size.times do |index|
		reverse_index = @project_modules.size - index.next
%>
<%= "\t" * reverse_index %>end<%
	end
%>
